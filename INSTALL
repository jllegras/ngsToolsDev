# for linux ubuntu (fedora use yum)
sudo apt-get install git-core gitk

git clone https://github.com/mfumagalli/ngsTools.git ngsTools # this creates a folder ngsTools/;  note that the ending .git is only for the bare repository in github

cd ngsTools

git pull

make clean

make

# ngsF is provided as a separate program. Source code and manual are available at https://github.com/fgvieira/ngsF.




